{
    "search.exclude": {
        "/bin/": true,
        "/docs/": true,
        "/lucee/": true,
        "/target/": true,
        "/temp/": true,
        "WEB-INF/": true
    },
    "java.project.sourcePaths": [
        "extension/src/main/java",
        "extension/src/test/java"
    ],
    "java.project.referencedLibraries": {
        "include" : [
            "extension/lib-lucee5/**/*.jar",
            "extension/lib/**/*.jar"
